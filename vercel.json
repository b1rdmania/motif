{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/src/server.ts"
    },
    {
      "src": "/health",
      "dest": "server/src/server.ts"
    },
    {
      "src": "/s/(.*)",
      "dest": "server/src/server.ts"
    },
    {
      "src": "/embed",
      "dest": "/embed.html"
    },
    {
      "src": "/models",
      "dest": "/models.html"
    },
    {
      "src": "/play",
      "dest": "/play.html"
    },
    {
      "src": "/ux-test",
      "dest": "/ux-test.html"
    },
    {
      "src": "/v2",
      "dest": "/v2.html"
    },
    {
      "src": "/v2-test",
      "dest": "/v2-test.html"
    },
    {
      "src": "/v2-diagnostic",
      "dest": "/v2-diagnostic.html"
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
